{
    "name": "apimediaru/nova-media-library",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-medialibrary": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "APIMedia\\NovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "APIMedia\\NovaMediaLibrary\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/apimediaru/laravel-medialibrary"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
